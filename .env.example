
# Database Configuration (Supabase)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_role_key_here

# Google OAuth Configuration (Optional)
GOOGLE_CLIENT_ID=your_google_oauth_client_id_here

# Server Configuration
NODE_ENV=production
PORT=5000

# Session Secret (IMPORTANT: Generate a strong random string)
SESSION_SECRET=your_random_session_secret_here_minimum_32_characters

# PostgreSQL Database (Alternative to Supabase)
DATABASE_URL=postgresql://username:password@hostname:port/database

# Optional: Additional Configuration
REPL_SLUG=your-app-name
REPL_OWNER=your-username

# Security Headers (Optional but recommended)
CORS_ORIGIN=https://yourdomain.com
